<aura:component Controller="BookAppointmentAndCancelled" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global">
    <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="accList" type="Object"/>
    <aura:attribute name="fieldMap" type="Map"/>
    <aura:attribute name="fieldMap1" type="Map"/>    
    <aura:attribute name="appointList" type="Appointment__c" default="{ 'Appointment__c': 'Name', 'Appointment_Date__c': '','Collection_type__c':'','Booking_Source__c':'' }"/>
    <aura:attribute name="selectedTime" type="String" />
    <aura:attribute name="picklistOptions" type="List" />
    <aura:attribute name="picklistCity" type="List" />
    
    
    <aura:html tag="style">
        .slds-modal__container{        
        width: 100% !important;
        max-width: 55rem !important;
        }
        table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
        border-right: 20px;
        padding-left: 60px;
        margin-top:20px;
        margin-bottom: 80px;        
        }
        
        td, th {
        border: none;
        text-align: left;
        padding: 10px;
        font-size: 14px;
        }
        
    </aura:html>
    
    
    <aura:handler name="init" value="{!this}" action="{!c.myAction}" />
  
    <div class="modal-header slds-modal__header font-size=20px;color: #2c276a;">
        <Center><h2 Style="font-size:20px;font-weight:bold;color: #140b61;">Book Appointment</h2></Center>  
    </div>
    
    <table>
        <tbody>
            <tr>
                <td><h2 style="font-size:12px;">Member Name </h2>
                    <h2  Style="margin-top:10px;">{!v.accList[0].Name}</h2>
                    <!--lightning:input type="text" label="Member Name" value="{!v.accList[0].Name}"/--> 
                </td>
                <td>
                    <lightning:select value="{!v.selectedTime}" label="Select City">
                        <aura:iteration items="{!v.picklistCity}" var="option">
                            <option value="{!option.value}">{!option.label}</option>
                        </aura:iteration>
                    </lightning:select>
                </td>
            </tr>	
            <tr>
                <td>
                    <lightning:select aura:id="industryPicklist" value="{!v.appointList.Collection_type__c}"  name="industryPicklist" label="Collection Type">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.fieldMap}" var="i" indexVar="key">
                            <option text="{!i.value}" value="{!i.key}" selected="{!i.key==v.appointList.Collection_type__c}" />
                        </aura:iteration>
                    </lightning:select>           
                </td>
                <td>
                    <lightning:input type="date" label="Select Appointment Date" value="{!v.appointList.Appointment_Date__c}" onchange="{!c.handleInputChange}" placeholder="Select Date"/>             
                </td>
            </tr>
            <tr>                
                <td>
                    <lightning:select value="{!v.selectedTime}" label="Select Time">
                        <aura:iteration items="{!v.picklistOptions}" var="option">
                            <option value="{!option.value}">{!option.label}</option>
                        </aura:iteration>
                    </lightning:select>
                </td>
                <td>
                    <lightning:input type="text" label="Select Address"/>             
                </td>                
            </tr>
            <tr>
                <td>
                    <lightning:input placeholder="test or package" aura:id="searchField" name="searchField" label="Search test or package"/>
                </td>
                <td>
                    <lightning:input type="text" placeholder="0" label="Total Amount" />             
                </td> 
            </tr>
            <tr>
                <td>
                    <lightning:input placeholder="0" aura:id="searchField" name="searchField" label="Pickup Charge"/>
                </td>
                <td>
                    <lightning:input type="text" placeholder="0" label="Grand Total" />             
                </td> 
            </tr>
            <tr>
                <td>
                    <lightning:input placeholder="A0000" name="searchField" label="Coupon Code"/>                    
                </td>
                <td>
                    <lightning:select aura:id="sourcetype" value="{!v.appointList.Booking_Source__c}" name="industryPicklist" label="Select Source Type">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.fieldMap1}" var="i" indexVar="key">
                            <option text="{!i.value}" value="{!i.key}" selected="{!i.key==v.appointList.Booking_Source__c}" />
                        </aura:iteration>
                    </lightning:select>                  
                </td> 
            </tr>
        </tbody>
    </table>
        
         <div class="slds-grid slds-gutters">
        <div class="slds-col">
            <center>
                <lightning:button variant="brand" label="Book"/>  
                <lightning:button variant="destructive" label="Cancel" onclick="{! c.doCancel }" class="slds-m-right_x-small"/>
            </center>
        </div>
    </div>
    
</aura:component>
