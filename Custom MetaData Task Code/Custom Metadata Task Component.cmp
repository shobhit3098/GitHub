<aura:component controller="MyContactListController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    
    <aura:handler name="init" value="{!this}" action="{!c.myAction}" />
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="Account" type="Account" />
    <aura:attribute name="Orderrs" type="Orderr__c" />
    <aura:attribute name="Appoinment" type="Appoinment__c" />
    <aura:attribute name="Label" type="List"/>
    <aura:attribute name="res" type="List"/>
    <aura:attribute name="Data" type="List"/>
    <aura:attribute name="columnsField" type="List"  />
    <aura:attribute name="Columns" type="List" />
    <aura:attribute name="Columns2" type="List" />

    <force:recordData aura:id="accountRecord"
                      recordId="{!v.recordId}"
                      targetFields="{!v.Account}"
                      layoutType="FULL"
                      />
    <lightning:card title="{!'Unique Code : ' + v.Account.Unique_Code__c}">
        <div style="margin: 18px; font-size:15px">
            <b >  Age :  </b>
            {!'' + v.Account.Age__c}

        </div>

        {!v.Label}
        
        <div >
        <lightning:datatable
                             keyField="id"
                             title="Custom"
                             data="{! v.Data }"
                             columns="{! v.columnsField }"
                             hideCheckboxColumn="true"/>
    </div>

        <!-- <lightning:card iconName="standard:contact" title="{! 'Contact List for ' + v.Account.Name}">  </lightning:card> -->
        <div style="margin: 20px">
            <b>{! 'Order List for ' + v.Account.Name}</b>
        </div>
        
        <div style="height:200px; ">
            <lightning:datatable data="{! v.Orderrs }" minColumnWidth="150" columns="{! v.Columns }" keyField="Id" hideCheckboxColumn="true"/>
        </div>
        <div style="height:62px;">
            <lightning:datatable data="{! v.Appoinment }" columns="{! v.Columns2 }" keyField="Id" hideCheckboxColumn="true"/>
        </div>
        
        
    </lightning:card>
</aura:component>


